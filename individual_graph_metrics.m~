G = {'0.6','0.7','0.775','0.6','0.475'};
Gidxs = [24;28;31;24;19];
idxs = [16;21;22;23;24;28;30;50;55;56;57;58;62;64];
subjs = {'EU','FJ','FI','DA','ER'};
fchcPaths = {'../../results/68/optimalG_EU_20120803_fic_dt1_sigma0.001_8mins_EI.mat',...
'../../results/68/optimalG_FJ_20120808_fic_dt1_sigma0.001_8mins_EI.mat',...
'../../results/68/optimalG_FI_20120727_fic_dt1_sigma0.001_8mins_EI.mat',...
'../../results/68/optimalG_DA_20120813_fic_dt1_sigma0.001_8mins_EI.mat',...
