schcPaths = {'../../data/connectivity_68/SC_FC_all/EU_20120803',...
             '../../data/connectivity_68/SC_FC_all/EU_20120803',...
             '../../data/connectivity_68/SC_FC_all/EU_20120803',...
             };
fcDistancePrior = zeros(5,68);
fcDistancePost = zeros(5,68);
partCoefSCEmp = zeros(5,68);
for i = 1:5
    t = load(fchcPaths{i},'fcs');
    fchc = t.fcs{Gidxs(i)};
    clear t;
    for j = 1:length(fcPathsFICPrior)
        fcPathPrior = strrep(fcPathsPrior{j}, '_G_', ['_G' G{i} '_']); 
        fcPathPost = strrep(fcPathsPost{j}, '_G_', ['_G' G{i} '_']);
        fcPrior = h5read(['../../results/68/' subjs{i} '/' fcPathPrior],'/CC');
        fcPost = h5read(['../../results/68/' subjs{i} '/' fcPathPost],'/CC');
        fcDistancePrior(i,j) = fc_distance(fcPrior,fchc,[j]);
        fcDistancePost(i,j) = fc_distance(fcPost,fchc,[j]);
    end
end